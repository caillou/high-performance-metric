{
  "name": "famigros-webapp",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:servers:next dev:servers:ssl-proxy",
