    "next-auth": "^4.22.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.2.0",
    "tsx": "^3.12.7",
    "usehooks-ts": "^2.9.1",
    "zod": "^3.21.4",
    "zod-formik-adapter": "^1.2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.4",
